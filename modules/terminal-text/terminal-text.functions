#!/bin/bash

function black 	{ echo -e "\033[0;30m$@\033[0m"; }
function blue 	{ echo -e "\033[0;34m$@\033[0m"; }
function green 	{ echo -e "\033[0;32m$@\033[0m"; }
function cyan 	{ echo -e "\033[0;36m$@\033[0m"; }
function red 	{ echo -e "\033[0;31m$@\033[0m"; }
function purple { echo -e "\033[0;35m$@\033[0m"; }
function orange { echo -e "\033[0;33m$@\033[0m"; }
function yellow { echo -e "\033[1;33m$@\033[0m"; }
function white 	{ echo -e "\033[1;37m$@\033[0m"; }

